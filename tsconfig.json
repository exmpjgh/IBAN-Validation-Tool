{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "module": "nodenext",
    "moduleResolution": "nodenext"
  },
  "exclude": ["node_modules", "**/*.json", "public/**/*"],
}
